
public class TicTacToe {

	public static void main(String[] args) {
		char[][] board = new char[5][5];
		int num = 1;
		
		for (int c = 0; c < board.length; c++) {
			if (c % 2 <= 1) {
				for (int i = 1; i <= board[c].length; i++) {
					if (i % 2 == 0) {
						board[c][i] = num;
						System.out.print("|");
					} else {
						board[c][i] = num;
					}
					num++
				}
			}
			
			System.out.println("");
		}
		
		for (int c = 0; r < board.length; r++) {
			for (int i = 0; c < board[r].length; c++) {
				System.out.print(grid[r][c] /*+ ""*/);
			}
			System.out.println("");
		}
	}

}
