
public class TicTacToe {

	public static void main(String[] args) {
		char[][] board = new char[5][5];
		int symNum = 0;
		char sym;
		//int num;
		
		for (int c = 0; c < board.length; c++) {
			if (c == 0 || c == 2 || c == 4) {
				sym = (char)symNum;
				for (int i = 0; i < board[c].length; i++) {
					if (i % 2 != 0){
						board[c][i] = '|';
					} else {
						board[c][i] = sym;
					}
				}
				symNum++;
			} else {
				for (int i = 0; i < board[c].length; i++) {
					board[c][i] = '-';
				}
			}
			
			
			System.out.println("");
		}
		
		
		for (int c = 0; c < board.length; c++) {
			for (int i = 0; i < board[c].length; i++) {
				System.out.print(board[c][i] + "");
			}
			System.out.println("");
		}
	}

}
